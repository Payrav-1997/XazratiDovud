@model Services.Hostory.Models.GetHistoryViewModel

@{
    ViewBag.Title = "title";
    Layout = null;
}
<head>
    <partial name="PartialCss" />
</head>
<body>


<div class="card component-card_9">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="~/admin/assets/img/600x300.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="~/admin/assets/img/600x300.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="~/admin/assets/img/600x300.jpg" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>                                            
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
           
    <div class="card-body">
        <h5 class="card-title" style="text-align: center">@Model.Title</h5>
        <p class="card-text">@Model.Description</p>
    </div>
</div>
<partial name="PartialJs" />
</body>
