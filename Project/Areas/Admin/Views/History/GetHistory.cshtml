@model Services.Hostory.Models.GetHistoryViewModel

@{
    ViewBag.Title = "title";
    Layout = null;
}
<head>
    <partial name="PartialCss" />
</head>
<body>

<a class="btn btn-danger" href="UpdateHistory/?id=@Model.Id">Изменить </a>

<div class="card component-card_9">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" style="height: 504px;" src="~/images/hazrati_dovud4.jfif" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" style="height: 504px;" src="~/images/JE0MJtcr483Fa7w3bZ1cZyUprykqLDzf.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" style="height: 504px;" src="~/images/m_27ad1f5b.jpg" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>                                            
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
           
    <div class="card-body">
        <h5 class="card-title" style="text-align: center">@Model.Title</h5>
        <p class="card-text">@Model.Description</p>
    </div>
</div>
<partial name="PartialJs" />
</body>
